        "Properties": {
            "isNegative": {
                "react": {
                    "isNegative": true
                },
                "css": "p-badge--negative"
            }
        }
    },
    "Blockquote": {
        "Components": {
            "css": "p-pull-quote",
            "html": "blockquote"
        },
        "Properties": {
            "Appearance": {
                "Default": {
                    "css": "p-pull-quote"
                },
                "Small": {
                    "css": "p-pull-quote--small"
                },
                "Large": {
                    "css": "p-pull-quote--large"
                }
            }
        }
    },
    "Breadcrumbs": {
        "Components": {
            "css": "p-breadcrumbs"
        }
    },
    "Button": {
        "Components": {
            "react": ["Button", "ActionButton", "ConfirmationButton", "SummaryButton"],
            "html": "button",
            "css": "p-button"
        },
        "Properties": {
            "Appearance": {
                "Default": {
                    "react": {
                        "appearance": ["", "Default"]
                    },
                    "html": "button",
                    "css": "p-button"
                },
                "Base": {
                    "react": {
                        "appearance": "Base"
                    },
                    "css": "p-button--base"
                },
                "Positive": {
                    "react": {
                        "appearance": "Positive"
                    },
                    "css": "p-button--positive"
                },
                "Negative": {
                    "react": {
                        "appearance": "Negative"
                    },
                    "css": "p-button--negative"
                },
                "Brand": {
                    "react": {
                        "appearance": "Brand"
                    },
                    "css": "p-button--brand"
                },
                "Link": {
                    "react": {
                        "appearance": "Link"
                    },
                    "css": "p-button--link"
                }
            },
            "Inline": {
                "react": {
                    "inline": true
                },
                "css": "is-inline"
            },
            "Dense": {
                "react": {
                    "dense": true
                },
                "css": "is-dense"
            },
            "Small": {
                "react": {
                    "small": true
                },
                "css": "is-small"
            },
            "Icon": {
                "react": {
                    "hasIcon": true
                },
                "css": "has-icon"
            }
        }
    },
